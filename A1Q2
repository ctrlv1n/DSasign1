#include <stdio.h>

void remove_duplicates(int a[], int *n) {
    int i, j, k;
    for (i = 0; i < *n; i++) {
        for (j = i+1; j < *n; ) {
            if (a[i] == a[j]) {
                // shift left
                for (k = j; k < *n - 1; k++) a[k] = a[k+1];
                (*n)--;
            } else j++;
        }
    }
}

int main() {
    int a[] = {2, 3, 2, 5, 3, 7, 7};
    int n = sizeof(a)/sizeof(a[0]), i;
    remove_duplicates(a, &n);
    printf("Array after removing duplicates: ");
    for (i = 0; i < n; i++) printf("%d ", a[i]);
    printf("\n");
    return 0;
}
